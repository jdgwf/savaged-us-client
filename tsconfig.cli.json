{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      // "jsx": "react",
      "lib": ["es6"],
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "./bin/",
      "target": "es6",
      "types": [
          "node"
      ]
    },
    "include": [
        "src/cli/*"
    ],
    "exclude": [
        "node_modules",
        "src/server/*",
        "src/react/*",
        "src/common/Pages/*",
        "src/common/Components/*",
    ],
}