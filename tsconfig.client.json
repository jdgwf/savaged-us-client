{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
    //   "module": "es6",
      // "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "./public/js/",
      "target": "es5",
    //   "noEmit": true,


        // "module": "esnext",

        // "target": "es6",
    //   "lib":[
    //       "es2017", "dom"
    //   ]
    "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ]
    },
    "include": [
        "src/react/**/*",
        "src/common/**/*"
    ],
    "exclude": [
        "node_modules",
        "src/server/**/*",
        "src/cli/**/*",
        "src/database/**/*",
    ],
  }